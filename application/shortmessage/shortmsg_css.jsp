<%
response.setContentType ("text/css");
 %> input {
	BORDER-RIGHT: #668235 1px solid;
	BORDER-TOP: #668235 1px solid;
	BORDER-LEFT: #668235 1px solid;
	BORDER-BOTTOM: #668235 1px solid;
	BACKGROUND-COLOR: #FAFBF5;
	font-size: 14px;
}

#shortmessage_vertmenu {
	float: left;
	height: 85%;
}

#shortmessage_vertmenu h1 {
	display: block;
	background-color: #FF9900;
	font-size: 90%;
	padding: 3px 0 5px 3px;
	border: 1px solid #000000;
	color: #333333;
	margin: 0px;
	width: 159px;
}

#shortmessage_vertmenu ul {
	list-style: none;
	margin: 0px;
	padding: 0px;
	border: none;
}

#shortmessage_vertmenu ul li {
	margin: 0px;
	padding: 0px;
}

#shortmessage_vertmenu ul li a {
	font-size: 80%;
	display: block;
	border-bottom: 1px dashed #C39C4E;
	padding: 5px 0px 2px 4px;
	text-decoration: none;
	color: #666666;
	width: 160px;
}

#shortmessage_vertmenu ul li a:hover,#shortmessage_vertmenu ul li a:focus
	{
	color: #000000;
	background-color: #ffffff;
}

#shortmessage_content {
	top: 0;
	float: center;
	width: 100%;
	height: 100%;
	background-color: #ffffff;
}

#shortmessage_content #inf_frame {
	width: 100%;
	height: 100%;
}

table.contacts {
	width: 580px;
	background-color: #fafafa;
	border: 1px #000000 solid;
	border-collapse: collapse;
	border-spacing: 0px;
}

td.contactDept {
	background-color: #FF9900;
	border: 1px #000000 solid;
	font-family: Verdana;
	font-weight: bold;
	font-size: 14px;
	color: #404040;
}

td.contact {
	border-bottom: 1px #6699CC dotted;
	text-align: center;
	font-family: Verdana, sans-serif, Arial;
	font-weight: normal;
	font-size: 14px;
	color: #404040;
	background-color: #ffffff;
	padding-top: 4px;
	padding-bottom: 4px;
	padding-left: 8px;
	padding-right: 0px;
}

td.contactlight {
	border-bottom: 1px #FF0000 dotted;
	text-align: center;
	font-family: Verdana, sans-serif, Arial;
	font-weight: normal;
	font-size: 14px;
	color: #000000;
	background-color: #cccccc;
	padding-top: 4px;
	padding-bottom: 4px;
	padding-left: 8px;
	padding-right: 0px;
}

#result ul {
	list-style: none;
	margin: 0px;
	padding: 0px;
	border: none;
}

#result ul li {
	margin: 0px;
	padding: 0px;
}

#result ul li a {
	font-size: 80%;
	display: block;
	border-bottom: 1px dashed #C39C4E;
	padding: 5px 0px 2px 4px;
	text-decoration: none;
	color: #666666;
	width: 160px;
}